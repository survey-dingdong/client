{
  "name": "@dingdong/design-system",
  "version": "0.0.1",
  "description": "Design system for DingDong",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/dingdong-ds.es.js",
      "require": "./dist/dingdong-ds.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "rm -rf dist && tsc && vite build",
    "clean:package": "yarn remove react-carousel-image-optimized && yarn cache clean && rm -rf ./react-carousel-image-optimized-v0.0.1.tgz",
    "add:package": "yarn add ./dingdong-ds-test-v0.1.0.tgz"
  },
  "resolutions": {
    "esbuild": "0.24.0"
  },
  "dependencies": {
    "@mui/material": "^6.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.8.0",
    "@eslint/js": "^9.9.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@storybook/addon-essentials": "8.4.2",
    "@storybook/addon-interactions": "8.4.2",
    "@storybook/addon-links": "8.4.2",
    "@storybook/addon-mdx-gfm": "8.4.2",
    "@storybook/addon-onboarding": "8.4.2",
    "@storybook/blocks": "8.4.2",
    "@storybook/react": "8.4.2",
    "@storybook/react-vite": "8.4.2",
    "@storybook/test": "8.4.2",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "esbuild": "0.21.5",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-storybook": "^0.8.0",
    "globals": "^15.9.0",
    "path": "^0.12.7",
    "rollup": "^4.34.9",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "storybook": "8.4.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
