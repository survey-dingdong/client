{
  "name": "dingdong-api-client",
  "version": "0.1.0",
  "description": "Survey DingDong API Client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "generate:api": "openapi-generator-cli generate -g typescript-axios -c openapi-config.yml -o src --template-dir ./my-templates && yarn run postprocess",
    "postprocess": "bash ./postprocess.sh",
    "prepare": "yarn build"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "typescript": "^5.3.0"
  },
  "license": "MIT"
}
